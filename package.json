{
    "name": "@craydent/craydent",
    "version": "0.9.0",
    "description": "node library based on craydent-1.9.3",
    "main": "index.js",
    "scripts": {
        "testTypes": "./local_scripts/testtypes.js",
        "testMain": "jasmine-node test/ --color --autotest --watch index.js --match -main-",
        "testNoConflict": "jasmine-node test/ --color --autotest --watch index.js --match -noconflict-",
        "gitResolve": "./local_scripts/gitResolve.sh",
        "pushPrep": "./local_scripts/gitprep.sh",
        "pushPublic": "git push public;",
        "push": "git push origin;",
        "pushAll": "git push origin; git push public;",
        "publishPublic": "./local_scripts/publish.sh",
        "publishPrivate": "./local_scripts/publishPrivate.sh",
        "buildDev": "./local_scripts/buildDev.sh",
        "build": "./local_scripts/build.sh",
        "generateTypes": "./local_scripts/generate_types.js",
        "test": "jest --coverage",
        "transpileDev": "./local_scripts/buildDev.js && tsc && ./local_scripts/copyPackageJsonFile.js",
        "transpile": "./local_scripts/buildProd.js && tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://cinada@bitbucket.org/craydent/node-library.git"
    },
    "keywords": [
        "craydent",
        "library",
        "jquery",
        "es6",
        "sugar",
        "utility",
        "ajax",
        "array",
        "class",
        "co",
        "control flow",
        "date",
        "express",
        "fs",
        "function",
        "http",
        "json",
        "koa",
        "moment",
        "mongo",
        "mongodb",
        "number",
        "object",
        "parser",
        "REST",
        "regexp",
        "route",
        "string",
        "swagger",
        "template",
        "typeof",
        "xml",
        "xml to json"
    ],
    "author": "Clark Inada",
    "license": "MIT",
    "homepage": "https://bitbucket.org/craydent/node-library#readme",
    "dependencies": {
        "@craydent/craydent-array": "*",
        "@craydent/craydent-class": "*",
        "@craydent/craydent-cli": "*",
        "@craydent/craydent-control-flow": "*",
        "@craydent/craydent-date": "*",
        "@craydent/craydent-fs": "*",
        "@craydent/craydent-function": "*",
        "@craydent/craydent-http": "*",
        "@craydent/craydent-json-parser": "*",
        "@craydent/craydent-number": "*",
        "@craydent/craydent-object": "*",
        "@craydent/craydent-regexp": "*",
        "@craydent/craydent-string": "*",
        "@craydent/craydent-template": "*",
        "@craydent/craydent-typeof": "*",
        "@craydent/craydent-utility": "*",
        "@craydent/craydent-xml-to-json": "*"
    },
    "devDependencies": {
        "@types/jest": "^26.0.3",
        "@types/node": "^11.9.4",
        "jest": "^26.1.0",
        "ts-jest": "^26.1.1",
        "typescript": "^3.9.6"
    },
    "jest": {
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "coveragePathIgnorePatterns": [
            "<rootDir>/transformed",
            "<rootDir>/tests"
        ],
        "testPathIgnorePatterns": [
            ".history",
            "__prototypes.ts",
            "index.template",
            "global.base.ts",
            "global.vars.ts"
        ],
        "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(ts)$",
        "moduleFileExtensions": [
            "js",
            "json",
            "node",
            "ts"
        ],
        "moduleNameMapper": {
            "^protected(.*)$": "<rootDir>/modules/protected$1",
            "^private(.*)$": "<rootDir>/modules/private$1",
            "^methods(.*)$": "<rootDir>/modules/methods$1",
            "^models(.*)$": "<rootDir>/modules/models$1"
        },
        "coverageDirectory": "<rootDir>/reports/code_coverage"
    }
}
